<?import javafx.scene.layout.StackPane?>
<?import sample.tabview.MainTab2?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.Priority?>
<?xml version="1.0" encoding="UTF-8"?>
<StackPane xmlns="http://javafx.com/javafx/8"
           xmlns:fx="http://javafx.com/fxml/1">
    <MainTab2 fx:id="preMainTab">

    </MainTab2>
    <HBox style="-fx-background-color:linear-gradient(#F1F5F9, #DFE5EC);
                -fx-border-width:1; -fx-border-color:#868E95;"
          maxHeight="35" StackPane.alignment="TOP_LEFT" alignment="CENTER_LEFT">
        <Button fx:id="preSave" styleClass="viewer_button">
            <graphic>
                <ImageView>
                    <Image url="@../images/save.png"/>
                </ImageView>
            </graphic>
            <HBox.margin>
                <Insets>
                    <left>10</left>
                </Insets>
            </HBox.margin>
        </Button>
        <Button fx:id="prePrint" styleClass="viewer_button">
            <graphic>
                <ImageView>
                    <Image url="@../images/print.png"/>
                </ImageView>
            </graphic>
            <HBox.margin>
                <Insets>
                    <left>10</left>
                </Insets>
            </HBox.margin>
        </Button>
        <HBox alignment="CENTER_RIGHT">
            <Button fx:id="preNewWindow" styleClass="viewer_button">
                <graphic>
                    <ImageView>
                        <Image url="@../images/newWindow.png"/>
                    </ImageView>
                </graphic>
                <HBox.margin>
                    <Insets>
                        <right>10</right>
                    </Insets>
                </HBox.margin>
            </Button>
            <HBox.hgrow>
                <Priority fx:constant="ALWAYS"></Priority>
            </HBox.hgrow>
        </HBox>
        <StackPane.margin>
            <Insets>
                <top>30</top>
            </Insets>
        </StackPane.margin>
    </HBox>
</StackPane>